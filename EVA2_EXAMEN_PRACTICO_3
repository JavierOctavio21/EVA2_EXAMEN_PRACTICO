                            boolean bGarg;
                            bGarg = input.nextBoolean();
                            if (bGarg == true) {
                                System.out.println("Posible diagnóstico: infección de garganta");
                            } else { System.out.println("¿Le duele la espalda por encima de las lumbrares, con escalofrios y fiebre?");
